@dominio = localhost
@port = 3000
@protocol = http
@url = {{protocol}}://{{dominio}}:{{port}}
### Login
POST {{url}}/api/user/login
Content-Type: application/json

{
  "email": "gerencia1",
  "password": "gerencia12345."
}


# {
#   "email": "root@root.adm",
#   "password": "root"
